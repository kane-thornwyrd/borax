{
  "name": "borax",
  "version": "0.0.3",
  "description": "Primitive flux in Backbone",
  "main": "lib/borax.min.js",
  "homepage": "https://github.com/kane-thornwyrd/borax",
  "bugs": {
    "url": "https://github.com/kane-thornwyrd/borax/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kane-thornwyrd/borax.git"
  },
  "dependencies": {
    "backbone": "^1.3.2",
    "bluebird": "^3.3.4",
    "jquery": "^2.2.2",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "chai": "^2.0.0",
    "coffee-script": "^1.9.0",
    "coveralls": "^2.11.2",
    "docco": "^0.7.0",
    "istanbul": "^0.3.8",
    "mocha": "^2.1.0",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "uglify-js": "^2.6.2"
  },
  "scripts": {
    "postinstall": "npm install -g mocha istanbul uglify-js docco chokidar-cli",
    "watch-tests": "chokidar 'tests/*.coffee' 'lib/*.js' -c 'npm test'",
    "watch-doc": "chokidar 'src/*.js' -c './node_modules/docco/bin/docco -o doc -t ./docco-theme/docco.jst -c ./docco-theme/docco.css src/*.js'",
    "watch-js": "chokidar 'src/*.js' -c 'uglify -s src/*.js -o lib/borax.min.js'",
    "test": "istanbul cover --print both node_modules/mocha/bin/_mocha -- --compilers coffee:coffee-script/register --reporter spec tests"
  },
  "author": {
    "name": "Jean-c√©dric THEROND",
    "email": "jean.cedric.t@gmail.com"
  },
  "license": "GPL-3.0",
  "readmeFilename": "README.md"
}
