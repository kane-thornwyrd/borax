  "homepage": "https://kane-thornwyrd.github.io/borax",
  "bugs": {
    "url": "https://github.com/kane-thornwyrd/borax/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kane-thornwyrd/borax.git"
  },
  "scripts": {
    "build": "rimraf lib && webpack --display-modules --display-chunks",
    "doc": "docco -o doc -- src/*.js",
    "fulltest": "npm run build && npm test",
    "lint": "eslint src/*.js",
    "test": "npm run lint && rimraf coverage && istanbul cover --print both _mocha -- --compilers coffee:coffee-script/register --reporter spec tests",
    "watch:doc": "chokidar 'src/*.js' -c 'npm run doc'",
    "watch:fulltest": "chokidar 'tests/*.coffee' 'src/*.js' -c 'npm run fulltest'",
    "watch:tests": "chokidar 'tests/*.coffee' -c 'npm test'",
    "watch:build": "chokidar 'src/*.js' -c 'npm run build'"
  },
  "dependencies": {
    "immutable": "^3.8.1"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-member-expression-literals": "^6.8.0",
    "babel-plugin-transform-merge-sibling-variables": "^6.8.0",
    "babel-plugin-transform-minify-booleans": "^6.8.0",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-plugin-transform-property-literals": "^6.8.0",
    "babel-plugin-transform-remove-console": "^6.8.0",
    "babel-plugin-transform-strict-mode": "^6.8.0",
    "babel-preset-es2015": "~6.3.13",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-preset-stage-3": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "chai": "^2.0.0",
    "chalk": "^1.1.3",
    "chokidar-cli": "*",
    "coffee-script": "^1.9.0",
    "coveralls": "^2.11.2",
    "docco": "^0.7.0",
    "eslint": "^2.11.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-cli": "^1.2.1",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.3",
    "gulp-util": "^3.0.7",
    "istanbul": "^0.3.8",
    "mocha": "^2.1.0",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "utils-merge": "^1.0.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0",
    "webpack": "^1.13.1"
  },
  "readmeFilename": "README.md"
}
